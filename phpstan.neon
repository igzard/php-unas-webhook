parameters:
  editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
  parallel:
    jobSize: 20
    maximumNumberOfProcesses: 32
    minimumNumberOfJobsPerProcess: 2
  phpVersion: 80499
  level: 5
  paths:
    - src
  bootstrapFiles:
    - vendor/autoload.php
  reportUnmatchedIgnoredErrors: false